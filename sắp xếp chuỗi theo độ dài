#include <stdio.h>
#include <string.h>

int main(){
    char name[100][100],i,k,swap[100],len;
    printf("Nhap so sinh vien <=100 \n");
    scanf("%d",&len);
    
	for(i=0;i<len;i++){
	   printf("Nhap ten sinh vien thu %d ",i+1);
	   scanf("%s",name[i]);
	}

	for(i=0;i<len-1;i++){
	 	for(k=i+1;k<len;k++){
		 	strcpy(swap,name[i]);	
		    if(strlen(name[i]) > strlen(name[k])){			   	
			   	strcpy(name[i],name[k]);
			   	strcpy(name[k],swap);
		 	}
		}
   }	
	printf("\nDo dai ten cua cac sinh vien duoc sap xep theo thu tu lon dan la \n");
	
	for(i=0;i<len;i++){
		printf("%s ",name[i]);
	}

}


